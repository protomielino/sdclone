<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ref-sector-p4" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="0" val="0"/>
	</section>
	<section name="Rear Wing">
		<attnum name="area" unit="m2" min="0.0" max="1.0" val="0.255"/>
		<attnum name="angle" unit="deg" min="0" max="22" val="6.0"/>
	</section>
	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-3"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0" max="5" val="4.04"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0" max="5" val="2.65"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.93"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.50"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0" max="5" val="1.18"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.92"/>
			</section>
			<section name="7">
				<attnum name="ratio" min="0" max="5" val="0.70"/>
			</section>
		</section>
	</section>
	<section name="Rear Differential">
		<attstr name="type" in="FREE" val="FREE"/>
		<attnum name="ratio" min="3.308" max="3.308" val="3.308"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="11000" val="11000"/>
	      <attnum name="brake correction" min="-0.5" max="0.5" val="0.14"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1" val="1"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1" val="1"/>
	</section>
	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="120" max="140" val="120"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
	</section>
	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="120" max="140" val="120"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
	</section>
	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="140" max="160" val="160"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2.5"/>
	</section>
	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="140" max="160" val="160"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2.5"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" unit="N/m" min="0" max="100000" val="55000"/>
		<attnum name="suspension course" unit="m" min="0.10" max="0.28" val="0.28"/>
		<attnum name="bellcrank" min="1" max="1" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="0" val="0"/>
		<attnum name="slow bump" unit="N/m/s" min="0" max="100000" val="5000"/>
		<attnum name="slow rebound" unit="N/m/s" min="0" max="100000" val="3000"/>
		<attnum name="fast bump" unit="N/m/s" min="0" max="100000" val="700"/>
		<attnum name="fast rebound" unit="N/m/s" min="0" max="100000" val="700"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" unit="N/m" min="0" max="100000" val="55000"/>
		<attnum name="suspension course" unit="m" min="0.10" max="0.28" val="0.28"/>
		<attnum name="bellcrank" min="1" max="1" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="0" val="0"/>
		<attnum name="slow bump" unit="N/m/s" min="0" max="100000" val="5000"/>
		<attnum name="slow rebound" unit="N/m/s" min="0" max="100000" val="3000"/>
		<attnum name="fast bump" unit="N/m/s" min="0" max="100000" val="700"/>
		<attnum name="fast rebound" unit="N/m/s" min="0" max="100000" val="700"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" unit="N/m" min="0" max="100000" val="70000"/>
		<attnum name="suspension course" unit="m" min="0.10" max="0.28" val="0.28"/>
		<attnum name="bellcrank" min="1" max="1" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="0" val="0"/>
		<attnum name="slow bump" unit="N/mn/s" min="0" max="100000" val="4000"/>
		<attnum name="slow rebound" unit="N/m/s" min="0" max="100000" val="5000"/>
		<attnum name="fast bump" unit="N/m/s" min="0" max="100000" val="700"/>
		<attnum name="fast rebound" unit="N/m/s" min="0" max="100000" val="700"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" unit="N/m" min="0" max="100000" val="70000"/>
		<attnum name="suspension course" unit="m" min="0.10" max="0.28" val="0.28"/>
		<attnum name="bellcrank" min="1" max="1" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="0" val="0"/>
		<attnum name="slow bump" unit="N/mn/s" min="0" max="100000" val="4000"/>
		<attnum name="slow rebound" unit="N/m/s" min="0" max="100000" val="5000"/>
		<attnum name="fast bump" unit="N/m/s" min="0" max="100000" val="700"/>
		<attnum name="fast rebound" unit="N/m/s" min="0" max="100000" val="700"/>
	</section>

	<section name="simplix private">
		<attnum name="genetic optimisation" min="0" max="1" val="0"/>
		<attnum name="initial fuel" min="-1" max="72.0" val="-1"/>

		<attnum name="scale brake" min="0" max="1" val="0.71"/>
		<attnum name="scale mu" min="0" max="2" val="0.71"/>
		<attnum name="scale bump" min="0" max="1" val="0.4"/>
		<attnum name="base mode" min="0.97" max="1.03" val="0.998"/>
		<attnum name="base scale" min="1.00" max="1.05" val="1.010"/>

		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="qualification" min="0" max="1" val="0"/>

		<attnum name="reserve" min="0.0" max="20000.0" val="2000"/>
		<attnum name="fuelper100km" min="1.0" max="100.0" val="70.0"/>

		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.03"/>
	</section>
</params>
